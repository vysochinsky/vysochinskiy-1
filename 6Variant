#include "pch.h"
#include <iostream>
#include <string>
#include <Windows.h>
using namespace std;

int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	setlocale(LC_ALL, "rus");
	string Text;
	string Text2;
	getline(cin,Text);
	for (int i = 0; i < Text.length(); i++)
	{
		if (Text[i] == ' ')
		{
			Text2 += Text[i];
		}
		Text2 += Text[i];
	}
	cout << Text2;
}

